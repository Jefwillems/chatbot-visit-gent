{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-console": "off",
    "no-restricted-syntax": "off",
    "no-use-before-define": "off",
    "guard-for-in": "off",
    "comma-dangle": "off",
    "camelcase": "off",
    "no-mixed-operators": "off",
    "no-underscore-dangle": "off",
    "max-len": "off"
  }
}
